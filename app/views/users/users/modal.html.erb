<% currently_at @user.user_name%>
<% provide :page_title, @user.user_name%>

<%= render TurboProfileComponent.new(title: 'About') do %>
    <% if @user.class.name == Company.name %>
        <%= render 'companies/shared/company_card', company: @user%>
    <% else %>
        <%= render 'clients/shared/about_client', client: @user%>
    <% end %>
<% end %>

<!-- Contacts company -->
<%= turbo_frame_tag dom_id(user, 'contacts'), class: "flex justify-center text-center" do %>
    <%= tag.div class: "flex justify-center items-center " do %>
        <%= link_to t('company.contacts'), user_contacts_path(user_id: user.id),
         class: "bottom-0 text-white bg-gradient-to-r from-green-500 via-green-600 to-green-700 hover:bg-gradient-to-br focus:ring-4 focus:outline-none focus:ring-green-800 font-medium rounded-lg text-sm px-5 py-2.5 text-center",
         data: {turbo_frame: user_signed_in? ? dom_id(user, 'contacts') : 'modal'} %>
    <% end %>
<% end %>
   